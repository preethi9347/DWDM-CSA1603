data<-c( 100, 70, 60, 90, 90 )
min_value<-min(data)
min_value
max_value<-max(data)
max_value
normalized_data_minmax<-(data-min_value)/(max_value-min_value)
normalized_data_minmax
mean<-mean(data)
mean
std_deviation<-sd(data)
std_deviation
normalized_data_zscore<-(data-mean)/std_deviation
normalized_data_zscore
